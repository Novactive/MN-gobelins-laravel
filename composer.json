{
  "name": "entrepreneur-interet-general/gobelins",
  "description": "Open access website for the Mobilier National collection.",
  "keywords": [
    "GLAM",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.4.8",
    "artesaos/seotools": "^0.18.0",
    "basemkhirat/elasticsearch": "dev-dev-scout-engine",
    "fideloper/proxy": "^4.4.0",
    "flynsarmy/csv-seeder": "^2.0.3",
    "folklore/image": "dev-master",
    "guzzlehttp/guzzle": "^7.0.1",
    "illuminated/console-logger": "^7.0",
    "kalnoy/nestedset": "^5.0",
    "laravel-lang/lang": "~7.0",
    "laravel/framework": "^7.0",
    "laravel/helpers": "^1.2",
    "laravel/scout": "^8.1.0",
    "laravel/tinker": "^2.4",
    "laravel/ui": "^2.0",
    "laravelium/sitemap": "7.*",
    "league/flysystem": "^1.0",
    "league/flysystem-memory": "^1.0",
    "nothingworks/blade-svg": "^0.3",
    "nyholm/psr7": "^1.3",
    "orangehill/iseed": "^2.6",
    "php-amqplib/php-amqplib": "^2.11",
    "picturae/oai-pmh": "^0.5.20",
    "platformsh/laravel-bridge": "^2.2",
    "predis/predis": "^1.1",
    "seld/signal-handler": "^1.2",
    "spatie/laravel-image-optimizer": "^1.6",
    "symfony/psr-http-message-bridge": "~2.0",
    "vladimir-yuldashev/laravel-queue-rabbitmq": "^10.0",
    "zendframework/zend-diactoros": "^2.2"
  },
  "require-dev": {
    "laravel/homestead": "^9",
    "barryvdh/laravel-debugbar": "^3.3.3",
    "composer/composer": "^1.9",
    "filp/whoops": "^2.7.3",
    "fzaninotto/faker": "^1.9.1",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^4.1",
    "phpunit/phpunit": "^8.5"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/nedbaldessin/elasticsearch"
    },
    {
      "type": "vcs",
      "url": "https://github.com/nedbaldessin/laravel-image"
    }
  ],
  "autoload": {
    "files": [
      "app/helpers.php",
      "app/platformsh-env.php"
    ],
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "folklore/image"
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-install-cmd": [
      "@php artisan optimize:clear",
      "@php artisan migrate --force",
      "@php artisan db:seed --class=ProductTypeSeeder",
      "@php artisan es:indices:create"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "build-assets": [
      "yarn install",
      "yarn run production"
    ],
    "platformsh-build": [
      "@build-assets"
    ],
    "platformsh-deploy": [
      "@post-install-cmd"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
